</head>
<body>
    <div class="wrapper">
        <div class="preloader"></div>

        <!-- Main Header Nav -->
        <header class="header-nav menu_style_home_one style2 navbar-scrolltofixed stricky main-menu">
            <div class="container-fluid p0">
                <!-- Ace Responsive Menu -->
                <nav>
                    <!-- Menu Toggle btn-->
                    <div class="menu-toggle">
                       <img class="nav_logo_img img-fluid" src="~/Content/images/header-logo.png" alt="header-logo.png">
                        <button type="button" id="menu-btn">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <a href="@Url.Action("Index","Home")" class="navbar_brand float-left dn-smd">
                        <img class="logo1 img-fluid" src="~/Content/images/header-logo2.png" alt="header-logo.png">
                        <img class="logo2 img-fluid" src="~/Content/images/header-logo2.png" alt="header-logo2.png">
                        <span>Emlak</span>
                    </a>
                    <!-- Responsive Menu Structure-->
                    <!--Note: declare the Menu style in the data-menu-style="horizontal" (options: horizontal, vertical, accordion) -->
                    <ul id="respMenu" class="ace-responsive-menu text-right" data-menu-style="horizontal">
                        <li>
                            <a href="@Url.Action("Index","Home")"><span class="title">Anasayfa</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Listing","Home")"><span class="title">İlanlar</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Blog","Home")"><span class="title">Blog</span></a>
                        </li>
                        <li>
                            <a><span class="title">Hakkımızda</span></a>
                            <ul>
                                <li><a href="@Url.Action("About","Home")">Hakkımızda</a></li>
                                <li><a href="@Url.Action("Sss","Home")">SSS</a></li>
                            </ul>
                        </li>
                        <li class="last">
                            <a href="@Url.Action("Contact","Home")"><span class="title">İletişim</span></a>
                        </li>
                        @if (Session["userid"] != null)
                        {
                            <li class="user_setting">
                                <div class="dropdown">
                                    <a class="btn dropdown-toggle" href="#" data-toggle="dropdown"><img class="rounded-circle" style="height:43px;width:43px" src="~/Content/uploads/avatar/@(Session["avatarim"] != null ? Session["avatarim"] : "User-01.png")" alt="emlak-user-photos"> <span class="dn-1199">@Session["kulAdSoyad"]</span></a>
                                    <div class="dropdown-menu">
                                        <div class="user_set_header">
                                            <img class="float-left" style="height:40px;width:40px" src="~/Content/uploads/avatar/@(Session["avatarim"] != null ? @Session["avatarim"]:"User-01.png")" alt="emlak-user-photos">
                                            <p>@Session["kulAdSoyad"] <br><span class="address"><a>@Session["kulEposta"]</a></span></p>
                                        </div>
                                        <div class="user_setting_content">
                                            <a class="dropdown-item active" href="@Url.Action("Membership","User")">Profilim</a>
                                            <a class="dropdown-item" href="@Url.Action("Messages","User")">Mesajlarım</a>
                                            <a class="dropdown-item" href="@Url.Action("MyAds","User")">İlanlarım</a>
                                            <a class="dropdown-item" href="@Url.Action("MyFavorites","User")">Favorilerim</a>
                                            <a class="dropdown-item" href="@Url.Action("Cikis","Giris")">Çıkış</a>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        }
                        else
                        {
                            <li class="list-inline-item list_s"><a href="#" class="btn flaticon-user" data-toggle="modal" data-target=".bd-example-modal-lg"> <span class="dn-lg">Giriş/Kayıt</span></a></li>
                        }

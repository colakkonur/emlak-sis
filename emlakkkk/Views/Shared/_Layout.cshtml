                        <div class="footer_menu_widget">
                            <ul>
                                <li class="list-inline-item"><a href="@Url.Action("Index","Home")">Anasayfa</a></li>
                                <li class="list-inline-item"><a href="@Url.Action("Listing","Home")">İlanlar</a></li>
                                <li class="list-inline-item"><a href="@Url.Action("Blog","Home")">Blog</a></li>
                                <li class="list-inline-item"><a href="@Url.Action("About","Home")">Hakkımızda</a></li>
                                <li class="list-inline-item"><a href="@Url.Action("Sss","Home")">SSS</a></li>
                                <li class="list-inline-item"><a href="@Url.Action("Contact","Home")">İletişim</a></li>

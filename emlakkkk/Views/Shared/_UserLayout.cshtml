<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords" content="advanced search custom, agency, agent, business, clean, corporate, directory, google maps, homes, listing, membership packages, property, real estate, real estate agent, realestate agency, realtor">
    <meta name="description" content="FindHouse - Real Estate HTML Template">
    <meta name="CreativeLayers" content="ATFN">
    <!-- css file -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link rel="stylesheet" href="~/Content/css/dashbord_navitaion.css">
    <!-- Responsive stylesheet -->
    <link rel="stylesheet" href="~/Content/css/responsive.css">
    <!-- Title -->
    <title>Emlak | Kullanıcı paneli</title>
    <!-- Favicon -->
    <link href="~/Content/images/favicon.ico" sizes="128x128" rel="shortcut icon" type="image/x-icon" />
    <link href="~/Content/images/favicon.ico" sizes="128x128" rel="shortcut icon" />
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="wrapper">
        <!-- Main Header Nav -->
        <header class="header-nav menu_style_home_one style2 menu-fixed main-menu">
            <div class="container-fluid p0">
                <!-- Ace Responsive Menu -->
                <nav>
                    <!-- Menu Toggle btn-->
                    <div class="menu-toggle">
                        <img class="nav_logo_img img-fluid" src="~/Content/images/header-logo.png" alt="header-logo.png">
                        <button type="button" id="menu-btn">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <a href="#" class="navbar_brand float-left dn-smd">
                        <img class="logo1 img-fluid" src="~/Content/images/header-logo2.png" alt="header-logo.png">
                        <img class="logo2 img-fluid" src="~/Content/images/header-logo2.png" alt="header-logo2.png">
                    </a>
                    <!-- Responsive Menu Structure-->
                    <!--Note: declare the Menu style in the data-menu-style="horizontal" (options: horizontal, vertical, accordion) -->
                    <ul id="respMenu" class="ace-responsive-menu text-right" data-menu-style="horizontal">
                        <li>
                            <a href="@Url.Action("Index","Home")"><span class="title">Anasayfa</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Listing","Home")"><span class="title">İlanlar</span></a>
                        </li>
                        <li>
                            <a href="@Url.Action("Blog","Home")"><span class="title">Blog</span></a>
                        </li>
                        <li>
                            <a><span class="title">Hakkımızda</span></a>
                            <ul>
                                <li><a href="@Url.Action("About","Home")">Hakkımızda</a></li>
                                <li><a href="@Url.Action("Sss","Home")">SSS</a></li>
                            </ul>
                        </li>
                        <li class="last">
                            <a href="@Url.Action("Contact","Home")"><span class="title">İletişim</span></a>
                        </li>
                        <li class="user_setting">
                            <div class="dropdown">
                                <a class="btn dropdown-toggle" href="#" data-toggle="dropdown"><img class="rounded-circle" style="height:43px;width:43px" src="~/Content/uploads/avatar/@(Session["avatarim"] != null ? Session["avatarim"] : "User-01.png")" alt="emlak-user-photos"> <span class="dn-1199">@Session["kulAdSoyad"]</span></a>
                                <div class="dropdown-menu">
                                    <div class="user_set_header">
                                        <img class="float-left" style="height:40px;width:40px" src="~/Content/uploads/avatar/@(Session["avatarim"] != null ? @Session["avatarim"]:"User-01.png")" alt="emlak-user-photos">
                                        <p>@Session["kulAdSoyad"] <br><span class="address"><a>@Session["kulEposta"]</a></span></p>
                                    </div>
                                    <div class="user_setting_content">
                                        <a class="dropdown-item active" href="@Url.Action("Membership","User")">Profilim</a>
                                        <a class="dropdown-item" href="@Url.Action("Messages","User")">Mesajlarım</a>
                                        <a class="dropdown-item" href="@Url.Action("MyAds","User")">İlanlarım</a>
                                        <a class="dropdown-item" href="@Url.Action("MyFavorites","User")">Favorilerim</a>
                                        <a class="dropdown-item" href="@Url.Action("Cikis","Giris")">Çıkış</a>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li class="list-inline-item add_listing"><a href="@Url.Action("Advertise","User")"><span class="flaticon-plus"></span><span class="dn-lg"> Ücretsiz ilan ver</span></a></li>
                    </ul>
                </nav>
            </div>
        </header>

        <!-- Main Header Nav For Mobile -->
        <div id="page" class="stylehome1 h0">
            <div class="mobile-menu">
                <div class="header stylehome1">
                    <div class="main_logo_home2 text-center">
                        <img class="nav_logo_img img-fluid mt20" src="~/Content/images/header-logo2.png" alt="header-logo2.png">
                        <span class="mt20">Emlak</span>
                    </div>
                    <ul class="menu_bar_home2">
                        <li class="list-inline-item list_s"><a href="page-register.html"><span class="flaticon-user"></span></a></li>
                        <li class="list-inline-item"><a href="#menu"><span></span></a></li>
                    </ul>
                </div>
            </div><!-- /.mobile-menu -->
            <nav id="menu" class="stylehome1">
                <ul>
                    <li><a href="@Url.Action("Index","Home")"><span class="title">Anasayfa</span></a></li>
                    <li><a href="@Url.Action("Listing","Home")"><span class="title">İlanlar</span></a></li>
                    <li><a href="@Url.Action("Blog","Home")"><span class="title">Blog</span></a></li>
                    <li><a href="@Url.Action("About","Home")"><span class="title">Hakkımızda</span></a></li>
                    <li><a href="@Url.Action("Sss","Home")"><span class="title">SSS</span></a></li>
                    <li><a href="@Url.Action("Contact","Home")">İletişim</a></li>
                    <li><a href="@Url.Action("Index","User")">Hesabım</a></li>
                    <li class="cl_btn"><a class="btn btn-block btn-lg btn-thm circle" href="@Url.Action("Advertise","User")"><span class="flaticon-plus"></span> Ücretsiz ilan ver</a></li>
                </ul>
            </nav>
        </div>

        <div class="dashboard_sidebar_menu dn-992">
            <ul class="sidebar-menu" style="width:17%">
                <li class="header" style="font-size:22px"><img src="~/Content/images/header-logo2.png" alt="header-logo2.png"> Emlak</li>
                <li class="title"><span>Genel</span></li>
                <li class="treeview"><a href="@Url.Action("Index","User")"><i class="flaticon-layers"></i><span> Dashboard</span></a></li>
                <li class="treeview"><a href="@Url.Action("Advertise","User")"><i class="flaticon-edit"></i><span> İlan Ver</span></a></li>
                <li class="title"><span>Bana Özel</span></li>
                <li class="treeview"><a href="@Url.Action("MyAds","User")"><i class="flaticon-home"></i> <span>İlanlarım</span></a></li>
                <li><a href="@Url.Action("MyFavorites","User")"><i class="flaticon-heart"></i> <span> Favorilerim</span></a></li>
                <li class="treeview"><a href="@Url.Action("Messages","User")"><i class="flaticon-envelope"></i><span> Mesajlarım</span></a></li>
                <li class="title"><span>Üyelik</span></li>
                <li><a href="@Url.Action("Membership","User")"><i class="flaticon-box"></i> <span>Üyelik Bilgilerim</span></a></li>
                <li><a href="@Url.Action("Cikis","Giris")"><i class="flaticon-logout"></i> <span>Çıkış</span></a></li>
            </ul>
        </div>

        <!-- Our Dashbord -->
        <section class="our-dashbord dashbord bgc-f7 pb50">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-3 col-xl-2 dn-992 pl0"></div>
                    <div class="col-lg-9 col-xl-10 maxw100flex-992">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="dashboard_navigationbar dn db-992">
                                    <div class="dropdown">
                                        <button onclick="myFunction()" class="dropbtn"><i class="fa fa-bars pr10"></i> Dashboard Navigation</button>
                                        <ul id="myDropdown" class="dropdown-content">
                                            <li class="@(ViewBag.Title == "Index" ? "active":"")"><a href="@Url.Action("Index","User")"><span class="flaticon-layers"></span> Dashboard</a></li>
                                            <li class="@(ViewBag.Title == "Advertise" ? "active":"")"><a href="@Url.Action("Advertise","User")"><span class="flaticon-edit"></span> İlan Ver</a></li>
                                            <li class="@(ViewBag.Title == "Messages" ? "active":"")"><a href="@Url.Action("Messages","User")"><span class="flaticon-envelope"></span> Mesajlarım</a></li>
                                            <li class="@(ViewBag.Title == "MyAds" ? "active":"")"><a href="@Url.Action("MyAds","User")"><span class="flaticon-home"></span> İlanlarım</a></li>
                                            <li class="@(ViewBag.Title == "MyFavorites" ? "active":"")"><a href="@Url.Action("MyFavorites","User")"><span class="flaticon-heart"></span> Favorilerim</a></li>
                                            <li class="@(ViewBag.Title == "Membership" ? "active":"")"><a href="@Url.Action("Membership","User")"><span class="flaticon-user"></span> Üyelik Bilgilerim</a></li>
                                            <li class="@(ViewBag.Title == "Cikis" ? "active":"")"><a href="@Url.Action("Cikis","Giris")"><span class="flaticon-logout"></span> Çıkış</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            @RenderBody()
                        </div>
                        <div class="row mt50">
                            <div class="col-lg-6 offset-lg-3">
                                <div class="copyright-widget text-center">
                                    <p>© @DateTime.Now.Year Emlak All rights reserved.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <a class="scrollToHome" href="#"><i class="flaticon-arrows"></i></a>
    </div>
    <!-- Wrapper End -->
    <script data-cfasync="false" src="~/Scripts/cloudflare-static/email-decode.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery-3.3.1.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery-migrate-3.0.0.min.js"></script>
    <script type="text/javascript" src="~/Content/js/popper.min.js"></script>
    <script type="text/javascript" src="~/Content/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.mmenu.all.js"></script>
    <script type="text/javascript" src="~/Content/js/ace-responsive-menu.js"></script>
    <script type="text/javascript" src="~/Content/js/chart.min.js"></script>
    <script type="text/javascript" src="~/Content/js/chart-custome.js"></script>
    <script type="text/javascript" src="~/Content/js/bootstrap-select.min.js"></script>
    <script type="text/javascript" src="~/Content/js/snackbar.min.js"></script>
    <script type="text/javascript" src="~/Content/js/simplebar.js"></script>
    <script type="text/javascript" src="~/Content/js/parallax.js"></script>
    <script type="text/javascript" src="~/Content/js/scrollto.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery-scrolltofixed-min.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.counterup.js"></script>
    <script type="text/javascript" src="~/Content/js/wow.min.js"></script>
    <script type="text/javascript" src="~/Content/js/progressbar.js"></script>
    <script type="text/javascript" src="~/Content/js/slider.js"></script>
    <script type="text/javascript" src="~/Content/js/timepicker.js"></script>
    <script type="text/javascript" src="~/Content/js/wow.min.js"></script>
    <script type="text/javascript" src="~/Content/js/dashboard-script.js"></script>
    <!-- Custom script for all pages -->
    <script type="text/javascript" src="~/Content/js/script.js"></script>
</body>
